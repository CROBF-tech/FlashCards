{
    "version": 2,
    "functions": {
        "api/index.js": {
            "maxDuration": 60,
            "memory": 1024
        }
    },
    "env": {
        "NODE_ENV": "production"
    },
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/api"
        }
    ]
}
